<?xml version="1.0"?>
<!-- -->

<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://chez.mario.fr/commande"
            xmlns="http://chez.mario.fr/commande"
            xmlns:plats="http://chez.mario.fr/plats"
            xmlns:clt="http://chez.mario.fr/client"
            elementFormDefault="qualified">
    
    <xsd:import namespace="http://chez.mario.fr/plats" schemaLocation="Plats.xsd"/>
    <xsd:import namespace="http://chez.mario.fr/client" schemaLocation="Client.xsd"/>
     
    <xsd:element name="commande" type="Commande"/>
 
    <xsd:complexType name="Commande">
        <xsd:sequence>
            <xsd:element name="client" type="clt:Client"/>
            <xsd:element name="plats"  type="plats:Plats" minOccurs="1" maxOccurs="unbounded"/>
            <xsd:element name="film"  type="Film" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="adresseLivraison" type="xsd:string"/>
        <xsd:attribute name="date"   type="xsd:string"/>
        <xsd:attribute name="id"   type="xsd:string"/>
        <xsd:attribute name="prix"   type="xsd:double"/>
    </xsd:complexType>

</xsd:schema>
