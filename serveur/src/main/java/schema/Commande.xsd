<?xml version="1.0"?>
<!-- -->

<xsd:schema version="1.0"
        
   
           xmlns:xsd="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://classesGen/commande"
           xmlns="http://classesGen/commande"
           xmlns:clt="classesGen/client"
           elementFormDefault="qualified">
    
    <xsd:import namespace="classesGen/client" schemaLocation="Client.xsd"/>
    <xsd:element name="commande" type="Commande"/>
    <xsd:complexType name="Commande">
        <xsd:sequence>   
        <xsd:element name="client" type="clt:Client"/>
        <xsd:element name="idPlat"  type="xsd:string" minOccurs="1" maxOccurs="unbounded"/>
        <xsd:element name="film"  type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attribute name="adresseLivraison" type="xsd:string"/>
        <xsd:attribute name="date"   type="xsd:string"/>
        <xsd:attribute name="id"   type="xsd:string"/>
        <xsd:attribute name="prix"   type="xsd:double"/>
    </xsd:complexType>

</xsd:schema>
