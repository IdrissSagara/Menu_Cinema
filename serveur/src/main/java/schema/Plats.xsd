<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://chez.mario.fr/plats"
            xmlns="http://chez.mario.fr/plats"
            xmlns:plat="http://chez.mario.fr/plat"
            elementFormDefault="qualified"> 
    <xsd:import namespace="http://chez.mario.fr/plat" schemaLocation="Plat.xsd"/>
    <xsd:element name="plats" type="Plats"/>
    <xsd:complexType name="Plats">
        <xsd:sequence>
            <xsd:element name="plat"  type="plat:Plat" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>   
    </xsd:complexType>
</xsd:schema>
